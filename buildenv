export ZOPEN_TARBALL_URL="http://ftp.gnu.org/gnu/screen/screen-4.9.0.tar.gz"
export ZOPEN_TARBALL_DEPS="curl make gzip tar ncurses m4 perl autoconf automake zoslib"
export ZOPEN_BOOTSTRAP="./autogen.sh"
export ZOPEN_TYPE="TARBALL"

zopen_check_results()
{
  dir="$1"
  pfx="$2"
  chk="$1/$2_check.log"

  # Echo the following information to guage build health
  echo "actualFailures:0"
  echo "totalTests:1"
  echo "expectedFailures:0"
}

zopen_append_to_env()
{
  # echo envars outside of PATH, MANPATH, LIBPATH
}
